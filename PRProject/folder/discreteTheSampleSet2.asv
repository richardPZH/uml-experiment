function [ ] = discreteTheSampleSet2( level )

load yeast.out

%discrete them
yeast = yeast';
[row col] = size( yeast );

maxArray = max( yeast );
minArray = min( yeast );

interVal = ( maxArray - minArray ) / level;

for k = 1:row
    for j = 1:col
        yeast( k , j ) = ( yeast( k , j ) - minArray( k ) ) / interVal(k);
    
    end
end


fid = fopen('yeast.dis2','w');
fprintf( fid,'%d\t%d\t%d\t%d\t%d\t%d\t%d\t%d\t%d\n', yeast );
fclose(fid);

end
